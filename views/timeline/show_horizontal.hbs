<aside>
  <h2>{{timeline.title}}</h2>
  <p>{{timeline.description}}</p>

</aside>
<style>
  section .containt .bars {
    width: 90%;
  }
</style>

<section>
  <div class="containt">
    <div class="bars">
      {{#each events}}
      <div class="bar {{this.color}} {{#unless this.endRow}}point{{/unless}}"
        style="grid-column-start: {{this.startRow}}; {{#if this.endRow}}grid-column-end: {{this.endRow}}{{/if}}">
        <div class="content">
          <div class="head">
            <h3>{{this.title}}</h3>
          </div>
          <div class="icon text-yellow">T</div>
          <h4>Category</h4>
          <p>{{this.content}}</p>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
</section>